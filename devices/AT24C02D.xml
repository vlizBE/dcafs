<commandset label="AT24C02D">
	<!-- NOTE1: Page Write need to stay within a page! (meaning only change least 3 significant bits-->
	<!-- NOTE2: Maximum amount of bytes read in a single command is 32 (library limit?) -->
	<command id="USB2514" info="Write settings for USB2514, do clear first.">
		<!-- 0x06 -> self,MTT,EOP off, individual cur sense -->
		<!-- 0x07 -> Dynamic power, compound, 8ms overcurrent, default:20 -->
		<!-- 0x09 -> Port 4 non-removable -->
		<!-- 0x13 -> Manufacturer:4B
			 0x14 -> product:6B
			 0x15 -> serial:2B -->
		<!-- Manufacturer: VLIZ -->	
		<write reg="0x00">0x24 0x04 0x14 0x26 0xA0 0x0B 0x9B 0xA4</write>
		<wait_ack>15</wait_ack>
		<write reg="0x08">0x00 0x10 0x00 0x00 0x01 0x32 0x01 0x32</write> 
		<wait_ack>15</wait_ack>
		<write reg="0x10">0x32 0x00 0x00 0x04 0x06 0x02 0x56 0x4C</write> 
		<wait_ack>15</wait_ack>
		<write reg="0x18">0x49 0x5A 0x00 0x00 0x00 0x00 0x00 0x00</write>
		<wait_ack>15</wait_ack>
		<write reg="0x50">0x00 0x00 0x00 0x00 0x44 0x41 0x53 0x48</write>
		<wait_ack>15</wait_ack>
		<write reg="0x58">0x55 0x42 0x00 0x00 0x00 0x00 0x00 0x00</write> <!-- Product: DASHUB -->
		<wait_ack>15</wait_ack>
		<write reg="0x92">0x00 0x01</write> <!-- serial: 1 -->
		<wait_ack>15</wait_ack>
		<write reg="0xFA">0x10</write> <!-- Swap D+ and D- for port 4 -->
	</command>
	<command id="readall" info="Read the content of all the registers">
		<read reg="0x00" return="32"/>
		<read reg="0x20" return="32"/>
		<read reg="0x40" return="32"/>
